@import '_normalize.styl'
@import '_typography.styl'
@import '_animations.styl'

html {
  font-size: 10px;
  font-family: sans-serif;
  line-height: 1;
}

p {
  font-size: 1.6rem;
  line-height: 1.5;
}

h2 {
  font-family: billabong, 'billabongregular'
  margin-left: 12px
  font-size: 2rem
}

.betslip-footer__sub__clearslip {
  font-family: billabong, 'billabongregular'
  font-size: 2rem
}

.navbar-brand {
  font-family: billabong, 'billabongregular'
  font-size: 4rem
}

h1 {
  font-family: billabong, 'billabongregular'
  text-align: left
  font-weight: 100
  font-size: 4rem
  letter-spacing: -1px
  margin-left: 12px;
  text-shadow: 0px 4px 0 rgba(18, 86, 136, 0.11)
}

body {
 color: #222
 font-family: Arial,Helvetica,"Helvetica Neue",sans-serif
 font-size: 16px
 background: #d9d9d9;
}

a {
 color: #248CB3;
 font-weight: 700;
 line-height: inherit;
}

.fl h2 {
 display: block
 padding-right: 10px;
 font-size 1.5em
 font-weight: bold
 -webkit-margin-before: 0.83em
 -webkit-margin-after: 0.83em
 -webkit-margin-start: 2px
 -webkit-margin-end: 2px
}

h3 {
 display: block
 font-size: 1.17em
 -webkit-margin-before: 1em
 -webkit-margin-after: 1em
 -webkit-margin-start: 0px
 -webkit-margin-end: 0px
 font-weight: bold
}

fl {
 float: left
}

aside, div, header, a, i, p, section, nav, span, ul, li, ol {
 margin: 0
 padding: 0
 border: 0
 font: inherit
 font-size: 100%
 vertical-align: baseline
}

aside, div, footer, header, main, menu, nav, section {
 display: block;
}

li {
 display: list-item;
}

ol, ul {
 list-style: none;
}

ul {
 display: block;
 margin-before: 1em;
 margin-after: 1em;
 margin-start: 0px;
 margin-end: 0px;
 padding-start: 40px;
}

.button, button-clear
 -moz-transition background-color .25s linear
 -o-transition background-color .25s linear
 -webkit-transition background-color .25s linear
